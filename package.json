{
  "name": "robe-react-commons",
  "version": "1.0.70",
  "description": "Robe React Commons",
  "main": "lib/index.js",
  "scripts": {
    "test_karma": "karma start config/webpack.config.test.js",
    "test": "node ./config/server/start.js 3000 & jest --config ./config/jest.json && killall node",
    "testd": "karma start config/webpack.config.test.dev.js",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "docs": "node ./config/prebuild.js ./docs & ./node_modules/.bin/esdoc -c esdoc.json",
    "build": "node ./config/prebuild.js ./lib & babel ./src --out-dir ./lib   --copy-files",
    "server": "node ./config/server/start.js 3000"
  },
  "keywords": [
    "webpack",
    "react",
    "es6",
    "eslint",
    "flowtype",
    "karma",
    "chai",
    "mocha",
    "isparta",
    "istanbul",
    "esdocs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robeio/robe-react-commons.git"
  },
  "license": "MIT",
  "devDependencies": {
    "app-root-path": "2.0.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
